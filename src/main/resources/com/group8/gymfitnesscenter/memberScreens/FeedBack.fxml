          <Label layoutX="157.0" layoutY="56.0" text="Rate your gym experience:" />
   
          <Slider fx:id="ratingSlider" layoutX="157.0" layoutY="85.0" majorTickUnit="1" max="5" min="1" prefHeight="62.0" prefWidth="303.0" showTickLabels="true" showTickMarks="true" />
   
          <Label layoutX="157.0" layoutY="150.0" text="Write your review:" />
       <TextArea fx:id="feedbackText" layoutX="157.0" layoutY="179.0" prefHeight="155.0" prefWidth="303.0" promptText="Enter your feedback here..." />
   
          <Button onAction="#submitFeedback" layoutX="161.0" layoutY="346.0" style="-fx-background-color: #4db8ff; -fx-text-fill: white;                    -fx-padding: 10 30; -fx-background-radius: 8;" text="Submit" />
      <Button layoutX="505.0" layoutY="22.0" mnemonicParsing="false" onAction="#goBack" prefHeight="27.0" prefWidth="77.0" text="back" />
   </children>
</AnchorPane>
