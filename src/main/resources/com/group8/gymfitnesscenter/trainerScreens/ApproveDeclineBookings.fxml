<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group8.gymfitnesscenter.Lamia.Trainer.ApproveDeclineBookingsController">

    <Label layoutX="180.0" layoutY="20.0" style="-fx-font-size: 24; -fx-font-weight: bold;" text="Approve / Decline Bookings" />


    <Label layoutX="40.0" layoutY="80.0" text="Pending Bookings:" />
    <ListView fx:id="bookingListView" layoutX="40.0" layoutY="110.0" prefHeight="220.0" prefWidth="350.0" />


    <Button layoutX="420.0" layoutY="140.0" onAction="#handleApprove" prefWidth="150.0" text="Approve" />

    <Button layoutX="420.0" layoutY="200.0" onAction="#handleDecline" prefWidth="150.0" text="Decline" />


    <Label fx:id="statusLabel" layoutX="40.0" layoutY="350.0" style="-fx-text-fill: green; -fx-font-size: 14;" />
   <Button layoutX="515.0" layoutY="32.0" mnemonicParsing="false" onAction="#goBackk" prefHeight="27.0" prefWidth="77.0" text="back" />

</AnchorPane>
